  <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-5">
         <div class="form-group">
            <div class="left-inner-addon">
               <i class="fa fa-user"></i>
               <%= f.hidden_field :quote_id, :value => airline_quote.id%>
                <%= f.text_field :name, class: "form-control", placeholder: "Your Name", required: true%>
            </div>
         </div>
      </div>
      <div class="col-xs-12 col-sm-12 col-md-5 pull-right">
         <div class="form-group">
            <div class="left-inner-addon">
               <i class="fa fa-envelope"></i>
              <%= f.text_field :email, class: "form-control",  placeholder: "Your Email", required: true%>
            </div>
         </div>
      </div>
      <div class="clearfix"></div>
      <div class="col-xs-12 col-sm-12 col-md-5">
         <div class="form-group">
            <div class="left-inner-addon">
               <i class="fa fa-building-o"></i>
              <%= f.text_field :company_name, class: "form-control", placeholder: "Company Name", required: true%>
            </div>
         </div>
      </div>
      <div class="col-xs-12 col-sm-12 col-md-5 pull-right">
         <div class="form-group">
            <div class="left-inner-addon">
               <i class="fa fa-building-o"></i>
              <%= f.text_field :phone_number, class: "form-control", id: "exampleInputAmount" , placeholder: "Phone No", required: true%>
            </div>
         </div>
      </div>
      <div class="clearfix"></div>
      <div class="col-xs-12 col-sm-12 col-md-5">
         <div class="form-group">
            <div class="left-inner-addon">
               <i class="fa fa-asterisk"></i>
               <%= f.text_field :hear, class: "form-control", placeholder: "How did you hear" %>
            </div>
         </div>
      </div>
      <div class="col-xs-12 col-sm-12 col-md-5 pull-right">
         <div class="form-group">
            <div class="left-inner-addon">
               <i class="fa fa-asterisk"></i>
               <%= f.text_field :uan_number, placeholder: "UAN Number ", class: "form-control"%>
            </div>
         </div>
      </div>
      <div class="clearfix"></div>
      <div class="col-xs-12 col-sm-12 col-md-5">
         <div class="form-group">
            <div class="left-inner-addon">
               <i class="fa fa-bars"></i>
                  <%= f.text_field :from_airpot, class: "form-control", placeholder: "Origin ", required: true%>
            </div>
         </div>
      </div>
      <div class="col-xs-12 col-sm-12 col-md-2 text-center">
         <h4>to</h4>
      </div>
      <div class="col-xs-12 col-sm-12 col-md-5 pull-right">
         <div class="form-group">
            <div class="left-inner-addon">
               <i class="fa fa-asterisk"></i>
                  <%= f.text_field :to_airpot, class: "form-control", placeholder: "Destination", required: true %>
            </div>
         </div>
      </div>
      <div class="clearfix"></div>
      <div class="info_area col-md-12">
         <label><strong>Freight information:</strong></label>
      </div>
      <div class="dimension_area">
         <div class="col-md-2 info_labal">
            <label>Dimensions</label>
         </div>
         <div class="col-md-3">
            <div class="form-group">
               <label>Length</label>
                  <%= f.text_field :cargo_dimension_length, class: "form-control" %>
            </div>
         </div>
         <div class="col-md-3">
            <div class="form-group">
               <label>Width</label>
                  <%= f.text_field :cargo_dimension_width, class: "form-control" %>
            </div>
         </div>
         <div class="col-md-3">
            <div class="form-group">
               <label>Height</label>
                  <%= f.text_field :cargo_dimension_height, class: "form-control" %>
            </div>
         </div>
      </div>
      <div class="clearfix"></div>
      <div class="width_area">
         <div class="col-md-2 info_labal">
            <label>width</label>
         </div>
         <div class="col-md-3">
            <div class="form-group">
               <label>Kgs</label>
                  <%= f.text_field :payload, class: "form-control" %>
            </div>
         </div>
         <div class="col-md-3">
            <div class="form-group">
               <label>Volume</label>
                  <%= f.text_field :volume, class: "form-control" %>
            </div>
         </div>
      </div>
      <div class="clearfix"></div>
      <div class="imageupload_area">
         <div class="col-md-5">
            <div class="form-group">
               <label>Number of palates/skids/PCs</label>
                  <%= f.text_field :quantiy, class: "form-control" %>
            </div>
         </div>
         <div class="col-md-1">
         </div>
         <div class="col-md-3">
            <div class="form-group">
               <label>Image</label>
               <input type="text" class="form-control" placeholder="  ">
            </div>
         </div>
         <div class="col-md-3">
            <div class="form-group">
               <a class="btn btn-default btn-upload" href="#" role="button">Upload</a>
            </div>
         </div>
      </div>
      <div class="clearfix"></div>
      <div class="datepicker_area">
         <div class="col-md-2 f_size17 ">
            <label>Date Required</label>
         </div>
         <div class="col-md-2">
            <div class="form-group">
               <label>Day</label>
               <input type="text" name="day" class="form-control" placeholder="  ">
            </div>
         </div>
         <div class="col-md-2">
            <div class="form-group">
               <label>Month</label>
               <input type="text" name="month" class="form-control" placeholder="  ">
            </div>
         </div>
         <div class="col-md-3">
            <div class="form-group">
               <label>year</label>
               <input type="text" name="year" class="form-control" placeholder="  ">
            </div>
         </div>
      </div>
      <div class="clearfix"></div>
      <div class="nature_area">
         <div class="head col-sm-12">
            <label><strong>Nature of Cargo</strong></label>
         </div>
         <div class="col-md-12">
            <%= f.select :nature_of_cargo, BrokerQuote::NATURE_OF_CARGO, {}, { :class => 'form-control' } %>
         </div>
         <div class="col-md-12  col-sm-12 col-xs-12 pull-right">
              <div class="col-md-6">
                  <div class="text_area">
                     <label>Additional Information <span>(optional field)</span> </label>
                     <%= f.text_area :additional_information, class: "form-control" %>
                 </div>
              </div>
              <div class="col-md-6">
                <div class="text_area">
                   <label>Special Requirements <span>(optional field)</span></label>
                      <%= f.text_area :special_requirements, class: "form-control" %>
                </div>
              </div>
         </div>
         <div class="btn_area">
              <%= f.submit button, class: "btn btn-default"%>
         </div>
      </div>
  </div>